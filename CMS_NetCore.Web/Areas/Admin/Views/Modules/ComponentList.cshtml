@model IEnumerable<CMS_NetCore.DomainClasses.Component>
<table class="table text-info">
    @foreach (var item in Model)
    {
        <tr>
            <td>
                <a asp-action="@item.AdminAction" asp-controller="@item.AdminController" asp-route-id="@item.ComponentId"><span class="text-info">@item.ComponentName</span> <span class="text-warning">@item.Descroption</span></a>
            </td>
        </tr>
    }
</table>
